      <providers>
        <add name="DefaultSessionProvider" type="System.Web.Providers.DefaultSessionStateProvider, System.Web.Providers, Version=2.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" connectionStringName="ColegioCN" />
      </providers>
    </sessionState>
  </system.web>
  <connectionStrings>
    <!--<add connectionString="Server=.\SQLExpress;Database=Colegio;User ID=colegio;Password=colegio" name="ColegioCN" providerName="System.Data.SqlClient" />-->
    <add connectionString="Server=(local);Database=MuseoCAS;User ID=colegio;Password=colegio" name="ColegioCN" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
    <system.webServer>
        <defaultDocument>
            <files>
